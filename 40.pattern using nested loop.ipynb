   "source": [
    "#pattern using  nested loop\n",
    "\n",
    "n=int(input(\"Enter the number:\"))\n",
    "for i in range (n):\n",
    "        for j in range (i):\n",
    "            print(\"*\",end=\" \")\n",
    "        print(\"\\n\")\n",
    "for i in range (n,0,-1):\n",
    "        for j in range (i):\n",
    "            print(\"*\",end=\" \")\n",
    "        print(\"\\n\")"
   ]
