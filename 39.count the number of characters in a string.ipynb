   "source": [
    "#count the number of characters in a string.\n",
    "n=(input(\"Enter the String:\"))\n",
    "c={}\n",
    "for i in n:\n",
    "    if i in c:\n",
    "        c[i]=c[i]+1\n",
    "    else:\n",
    "        c[i]=1\n",
    "for keys,value in c.items():\n",
    "    print(keys,'is repeated',value,'times')\n",
    "\n"
   ]
