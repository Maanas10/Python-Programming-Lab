    "#56. Sort dictionary in ascending and descending order.\n",
    "a={'saji':12,'ravi':13,'abhi':15,'biju':14} \n",
    "print(\"Dictionary\",a)\n",
    "l=list(a.items())\n",
    "l.sort()            \n",
    "print('Ascending order:',l) \n",
    "l=list(a.items())\n",
    "l.sort(reverse=True) \n",
    "print('Descending order:',l)\n"
