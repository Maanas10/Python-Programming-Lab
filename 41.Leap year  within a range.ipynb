   "source": [
    "#Leap year within a range\n",
    "\n",
    "a=int(input(\"Enter the starting year limit:\"))\n",
    "b=int(input(\"Enter the Ending year limit:\"))\n",
    "print(\"The leap year between %d and %d are\" %(a,b))\n",
    "for i in range(a,b):\n",
    "    if(i%4==0):\n",
    "        if(i%100!=0 or i%400==0):\n",
    "            print(i)"
