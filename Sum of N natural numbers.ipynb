    "#Sum of natural numbers\n",
    "n=int(input(\"Enter the number:\"))\n",
    "i=1\n",
    "sum=0\n",
    "while(i<=n):\n",
    "    sum=sum+i\n",
    "    i=i+1\n",
    "print(\"The sum of %d natural numbers is %d\" %(n,sum))"
   ]
