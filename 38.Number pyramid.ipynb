    "# Display the given pyramid with step number accepted from user.\n",
    "n=int(input(\"Enter the number:\"))\n",
    "\n",
    "\n",
    "for i in range (1,n+1):\n",
    "    \n",
    "    for j in range (1,i+1):\n",
    "        print(j*i,end=\" \")\n",
    "    print(\"\\n\")"
   ]
