   "source": [
    "#Calculate the average of numbers in tuple\n",
    "\n",
    "a=tuple(input(\"Enter the numbers seperated by comma:\").split(\",\"))\n",
    "s=len(a)\n",
    "v=0\n",
    "for i in range (s):\n",
    "    v=int(a[i])+v\n",
    "    e=v/s\n",
    "print(\"The average of the numbers in tupples is:\",e)"
